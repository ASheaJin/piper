<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.syswin.com.syswin.pipeline.db.repository.UserlogRepository">
    <resultMap id="BaseResultMap" type="com.syswin.com.syswin.pipeline.db.model.Userlog">
        <result column="user_id" jdbcType="VARCHAR" property="userId"/>
        <result column="request_data" jdbcType="VARCHAR" property="requestData"/>
        <result column="create_time" jdbcType="VARCHAR" property="createTime"/>
    </resultMap>

    <sql id="Base_Column_List" >
       user_id, request_data, create_time
    </sql>

    <select id="selectByIdTime" resultType="com.syswin.com.syswin.pipeline.db.model.Userlog">
        select
        <include refid="Base_Column_List"/>
        from server_logs
        where
        1=1
        <if test="userId != ''">
            and user_id = #{userId}
        </if>
        <if test="startTime != 0">
            and create_time/1000 > #{startTime}
        </if>
        <if test="endTime != 0">
            and #{endTime} > create_time/1000
        </if>
        <if test="end != 0">
            limit #{start},#{end}
        </if>
    </select>
    <select id="getCount" resultType="java.lang.Integer">
        select
        count(1)
        from server_logs
    </select>
</mapper>